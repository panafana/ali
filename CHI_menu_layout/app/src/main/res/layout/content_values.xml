<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".Values"
    tools:showIn="@layout/activity_values"
    android:background="#11FFFF">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Football news"
        android:id="@+id/textView2"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <Button


        android:id="@+id/button"
        android:layout_width="85dp"
        android:layout_height="60dp"
        android:drawableStart="@drawable/uk"
        android:background="@android:color/transparent"
        android:paddingLeft="7dp"
        android:layout_marginTop="46dp"
        android:layout_below="@+id/textView2"
        android:layout_centerHorizontal="true" />

    <Button


        android:layout_width="85dp"
        android:layout_height="60dp"
        android:drawableStart="@drawable/spain"
        android:background="@android:color/transparent"
        android:paddingLeft="7dp"
        android:id="@+id/button3"
        android:layout_marginTop="46dp"
        android:layout_below="@+id/button"
        android:layout_alignStart="@+id/button" />

    <Button

        android:layout_width="85dp"
        android:layout_height="60dp"
        android:drawableStart="@drawable/germany"
        android:background="@android:color/transparent"
        android:paddingLeft="7dp"

        android:id="@+id/button4"
        android:layout_marginTop="58dp"
        android:layout_below="@+id/button3"
        android:layout_alignStart="@+id/button5" />

    <Button

        android:layout_width="85dp"
        android:layout_height="60dp"
        android:drawableStart="@drawable/italy"
        android:background="@android:color/transparent"
        android:paddingLeft="7dp"
        android:id="@+id/button5"
        android:layout_marginTop="48dp"
        android:layout_below="@+id/button4"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView4"
        android:layout_below="@+id/textView2"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textView5"
        android:layout_below="@+id/textView4"
        android:layout_alignLeft="@+id/button"
        android:layout_alignStart="@+id/button" />
</RelativeLayout>
